    <script>   
        $(function(){
            var arry
            var listText;
            $(".titList .list li").click(function(){
                listText = $(this).text();
                clickP();
            });
            var check;
            $(".check input").click(function(){
                check = $(".check input:checked");
                clickP();
            })
            var cate01 = $(".subTxt01").text();
            function  clickP(){     
                if (listText !== undefined && check !== undefined){
                     var totalResult = {
                        listText : listText,
                        check : check
                    }
                    typeListAll(totalResult);
                } else if (listText !== undefined){
                    typeList(listText);
                } else if (check !== undefined){
                     var totalResult = {
                        cate01 : cate01,
                        check : check
                    }
                    typeListC(totalResult);
                } 
            }
            
            var size = $(".likeN").length;
            var productSize = $(".productN").length;
            for(var i=0; i<size; i++){
                var likeN = $(".likeN").eq(i).text();
                for(var k=0; k<productSize; k++){
                    var productN = $(".productN").eq(k).text();
                    var productPoint = $(".productN").eq(k).prev().prev().prev().children().first();
                    if (likeN === productN){
                       $(productPoint).addClass("onStar")
                    } 
                }
            }
        });
    </script>
